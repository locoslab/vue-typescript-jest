{
  "name": "vue-typescript-jest",
  "version": "0.2.2",
  "description": "Jest preprocessor.js for Vue.js components and TypeScript",
  "author": "Locoslab GmbH",
  "homepage": "https://github.com/locoslab/vue-typescript-jest",
  "bugs": "https://github.com/locoslab/vue-typescript-jest/issues",
  "repository": "locoslab/vue-typescript-jest",
  "license": "MIT",
  "main": "./preprocessor.js",
  "files": [
    "preprocessor.js"
  ],
  "keywords": [
    "jest",
    "vue",
    "vueify",
    "typescript",
    "pug"
  ],
  "scripts": {
    "test": "jest",
    "prepublish": "eslint preprocessor.js && jest --no-cache --coverage"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "moduleFileExtensions": [
      "ts",
      "js",
      "vue"
    ],
    "testRegex": "/test/test/.*\\.(ts|js)$",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/test/.*\\.(ts|js)$",
      "/.*\\.vue"
    ]
  },
  "devDependencies": {
    "@types/jest": "^15.1.32",
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "eslint": "^3.7.0",
    "eslint-config-locoslab": "github:locoslab/eslint-config-locoslab",
    "html2jade": "^0.8.4",
    "jest-cli": "^16.0.1",
    "pug": "^2.0.0-beta6",
    "typescript": "^2.0.3",
    "vue": "^2.0.1",
    "vue-template-compiler": "^2.0.1",
    "vue-typescript-import-dts": "^1.0.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "locoslab",
    "env": {
      "node": true
    }
  }
}
